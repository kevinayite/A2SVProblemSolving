from collections import Counter
class Solution:
    def commonChars(self, words: List[str]) -> List[str]:
        dict_primary = Counter(words[0])

        for i in range(1, len(words)):
            dict_primary &= Counter(words[i])# Let me explain how this part of the code will behave. & works as intersection 
          # here as we have a dictionary it will keep tracking the minimum                                              
        return list(dict_primary.elements()) # print elements of the dictionary time their value;
        
